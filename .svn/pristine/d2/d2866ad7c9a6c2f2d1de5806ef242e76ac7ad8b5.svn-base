package site.itwill.exception;

import site.itwill.dto.Show;

public class ShowExistsException extends Exception {
	private static final long serialVersionUID = 1L;
	
	private Show show;
	
	public ShowExistsException() {
		// TODO Auto-generated constructor stub
	}

	public ShowExistsException(String message, Show show) {
		super(message);
		this.show = show;
	}

	public Show getShow() {
		return show;
	}

	public void setShow(Show show) {
		this.show = show;
	}
}
